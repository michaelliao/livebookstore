INSERT INTO T_BOOK VALUES('8a41b5c10fd7331e010fd7460b340006','\u8d3e\u6829\u6df3',17039616,'\u7535\u5b50\u5de5\u4e1a\u51fa\u7248\u793e',NULL,72,'7121033232','2006-11-01',35.0E0,0,0,0,12,'Photoshop\u5e73\u9762\u8bbe\u8ba1\u5e08\u653b\u7565','\u4e2d\u6587',0,'\u672c\u4e66\u662f\u4e13\u4e1a\u4ecb\u7ecdPhotoshop\u5728\u8bbe\u8ba1\u9886\u57df\u4e2d\u5177\u4f53\u5e94\u7528\u7684\u5b9e\u4f8b\u6559\u7a0b\u3002\u5168\u4e66\u4ee5Photoshop\u4e3a\u57fa\u7840\uff0c\u9996\u5148\u7efc\u5408\u4ecb\u7ecd\u4e86\u8bbe\u8ba1\u7684\u5e73\u9762\u6784\u6210\u548c\u8272\u5f69\u6784\u6210\u5e38\u8bc6\u4e0e\u8bbe\u8ba1\u65b9\u6cd5\uff0c\u7136\u540e\u4ecb\u7ecd\u4e86\u5e73\u9762\u8bbe\u8ba1\u5e38\u7528\u8f6f\u4ef6Photoshop\u7684\u4f7f\u7528\u65b9\u6cd5\uff0c\u6700\u540e\u5728\u5b9e\u6218\u7c7b\u4e2d\u5217\u4e3e\u4e867\u4e2a\u5927\u578b\u5b9e\u4f8b\uff0c\u4ecb\u7ecd\u76f8\u5173\u8bbe\u8ba1\u7684\u521b\u610f\u65b9\u6cd5\u3001\u5236\u4f5c\u65b9\u6cd5\u548c\u8be6\u5c3d\u7684\u5236\u4f5c\u6b65\u9aa4\u3002\u000d<br/>\u5168\u4e66\u5185\u5bb9\u4e30\u5bcc\u3001\u5b9e\u4f8b\u7ecf\u5178\u3001\u6280\u672f\u5b9e\u7528\u3001\u6b65\u9aa4\u8be6\u5c3d\uff0c\u529b\u6c42\u901a\u8fc7\u5b9e\u4f8b\u7684\u4ecb\u7ecd\u4f7f\u8bfb\u8005\u638c\u63e1\u8bbe\u8ba1\u5e38\u8bc6\uff0c\u719f\u7ec3\u8f6f\u4ef6\u64cd\u4f5c\uff0c\u8ba9\u8bfb\u8005\u7684\u5b66\u4e60\u66f4\u52a0\u8d34\u8fd1\u5b9e\u9645\uff0c\u5c06Photoshop\u7684\u5b9e\u7528\u6280\u672f\u6dcb\u6f13\u5c3d\u81f4\u5730\u5448\u73b0\u7ed9\u8bfb\u8005\u3002\u000d<br/>\u672c\u4e66\u65e2\u53ef\u4ee5\u8ba9\u521d\u5b66\u8005\u5feb\u901f\u9886\u609fPhotoshop\u5728\u8bbe\u8ba1\u4e2d\u7684\u7cbe\u9ad3\u5185\u5bb9\uff0c\u53ef\u4ee5\u8ddf\u968f\u672c\u4e66\u7684\u6b65\u9aa4\u5b8c\u6210\u5b66\u4e60\uff0c\u53c8\u53ef\u4ee5\u7ed9\u8fdb\u9636\u8005\u4ee5\u542f\u53d1\u4e0e\u53c2\u8003\u3002\u000d<br/>\u5168\u4e66\u9002\u5408\u6b32\u4ece\u4e8b\u5e7f\u544a\u8bbe\u8ba1\u5de5\u4f5c\u7684\u8bfb\u8005\u4f7f\u7528\u3002\u53e6\u5916\uff0c\u6b64\u4e66\u4e5f\u53ef\u4f5c\u4e3a\u793e\u4f1a\u57f9\